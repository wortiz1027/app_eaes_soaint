  Author:  negro
  Type: BPEL 2.0 Process
  Purpose: Synchronous BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="bpel_consulta_wcc"
               targetNamespace="http://xmlns.oracle.com/app_eaes_soaint/SOA_Content/bpel_consulta_wcc"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/app_eaes_soaint/SOA_Content/bpel_consulta_wcc"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:ui="http://xmlns.oracle.com/soa/designer"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://www.stellent.com/Search/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns2="http://xmlns.javeriana.edu.co/co/schemas/process/bpm/WCC_Search/v1.0">

    <import ui:processWSDL="true" namespace="http://xmlns.oracle.com/app_eaes_soaint/SOA_Content/bpel_consulta_wcc" location="../WSDLs/bpel_consulta_wcc.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="bpel_consulta_wcc_client" partnerLinkType="client:bpel_consulta_wcc" myRole="bpel_consulta_wccProvider"/>
    <partnerLink name="WS_Search" partnerLinkType="ns1:WS_Search" partnerRole="SearchSoap"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:bpel_consulta_wccRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:bpel_consulta_wccResponseMessage"/>
    <variable name="Ink_consultar_wcc_input" messageType="ns1:QuickSearchSoapIn"/>
    <variable name="Ink_consultar_wcc_output" messageType="ns1:QuickSearchSoapOut"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in bpel_consulta_wcc.wsdl) -->
    <receive name="receiveInput" partnerLink="bpel_consulta_wcc_client" portType="client:bpel_consulta_wcc" operation="process" variable="inputVariable" createInstance="yes"/>
    <assign name="Set_Request">
      <copy>
        <from>$inputVariable.payload/ns2:queryText</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Ink_consultar_wcc_input.parameters/ns1:queryText</to>
      </copy>
      <copy>
        <from>$inputVariable.payload/ns2:property/ns2:name</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Ink_consultar_wcc_input.parameters/ns1:extraProps/ns1:property/ns1:name</to>
      </copy>
      <copy>
        <from>$inputVariable.payload/ns2:property/ns2:value</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Ink_consultar_wcc_input.parameters/ns1:extraProps/ns1:property/ns1:value</to>
      </copy>
      <copy>
        <from>$inputVariable.payload/ns2:property</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Ink_consultar_wcc_input.parameters/ns1:extraProps/ns1:property</to>
      </copy>
    </assign>
    <invoke name="Ink_consultar_wcc" partnerLink="WS_Search" portType="ns1:SearchSoap"
            operation="QuickSearch" inputVariable="Ink_consultar_wcc_input" outputVariable="Ink_consultar_wcc_output"
            bpelx:invokeAsDetail="no"/>
    <assign name="T1_Response_Search">
      <bpelx:annotation>
        <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from>ora:doXSLTransformForDoc("../Transformations/T1_Response_Search.xsl", $Ink_consultar_wcc_output.parameters)</from>
        <to variable="outputVariable" part="payload"/>
      </copy>
    </assign>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="bpel_consulta_wcc_client" portType="client:bpel_consulta_wcc" operation="process" variable="outputVariable"/>
  </sequence>
</process>